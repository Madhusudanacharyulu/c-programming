#include <stdio.h>
int main()
{
    int i, n;
    printf("Print odd numbers from: ");
    scanf("%d",&i);
    printf("Print odd numbers till: ");
    scanf("%d", &n);
    printf("All odd numbers from %d to %d are: \n",i, n);

    /* Start loop from 1 and increment it by 1 */
    for(i;i<=n; i++)
    {
        /* If 'i' is odd then print it */
        if(i%2!=0)
        { 
            if(i==3)
            {
                continue;
            }
            printf("%d\n", i);
        }
    }
    return 0;
}
